sim_widget_data <- function(N, noise=100){
N=1000
x_temp <- rnorm(N,mean = 10,sd = 5)
x<-x_temp
x[which(x_temp <0)]<-x_temp[which(x_temp <0)]*-1
x[which(x_temp >20)]<-20-x_temp[which(x_temp >20)]
y <-  - x/2 + 100 + rnorm(N, sd=noise)
z<-runif(N)
bad_widget <- factor(y > 140) #original
data.frame(x, y,z, bad_widget)
}
widget_data <- sim_widget_data(1000, 0)
plot(widget_data$x,widget_data$y)
